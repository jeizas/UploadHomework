<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.spring.dao.AdminMapper" >
  <resultMap id="BaseResultMap" type="com.spring.model.Admin" >
    <id column="ano" property="ano" jdbcType="CHAR" />
    <result column="apwd" property="apwd" jdbcType="CHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    ano, apwd
  </sql>
  
  <select id="getAll" resultMap="BaseResultMap">
  	select *
  	from admin;
  </select>
  
  <update id="updateBykey" parameterType="java.lang.String">
  	update admin
  	set apwd = #{1}
  	where ano = #{0}
  </update>
  
</mapper>